{
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#3a816f",
    "activityBar.background": "#3a816f",
    "activityBar.foreground": "#fbfbfb",
    "activityBar.inactiveForeground": "#fbfbfb",
    "activityBarBadge.background": "#3979e7",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#3a816f",
    "sash.hoverBorder": "#fbfbfb",
    "statusBar.background": "#3a816f",
    "statusBar.foreground": "#fbfbfb",
    "statusBarItem.hoverBackground": "#000000",
    "statusBarItem.remoteBackground": "#000000",
    "statusBarItem.remoteForeground": "#3a816f",
    "titleBar.activeBackground": "#fbfafa",
    "titleBar.activeForeground": "#000000",
    "titleBar.inactiveBackground": "#000000",
    "titleBar.inactiveForeground": "#FFE4BF"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit"
  },
  "files.trimTrailingWhitespace": true,
  "files.trimFinalNewlines": true,
  "files.insertFinalNewline": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.eol": "\n"
}
